    "while(True):\n",
    "    a,b,c = map(int,input().split())\n",
    "    if a==0 and b==0 and c == 0:\n",
    "        break\n",
    "    if (b-a) == (c-b) and (b-a != 0):\n",
    "        print(\"AP\", c+(c-b))\n",
    "    else:\n",
    "        print(\"GP\", int(c*(c/b)))"
   ]
